<Window x:Class="TheBillionApp.AdminPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheBillionApp"
        mc:Ignorable="d"
        Title="AdminPanel" Height="600" Width="600">
    <Grid>
        <DataGrid x:Name="grid1" HorizontalAlignment="Left" Height="161" Margin="304,10,0,0" VerticalAlignment="Top" Width="278"/>
        <TextBox x:Name="txtServicio" HorizontalAlignment="Left" Height="32" Margin="10,10,0,0" TextWrapping="Wrap" Text="servicio" VerticalAlignment="Top" Width="264" FontSize="25" TextChanged="TextBox_TextChanged_1"/>
        <TextBox x:Name="txtNServicio" HorizontalAlignment="Left" Height="32" Margin="10,47,0,0" TextWrapping="Wrap" Text="nombre" VerticalAlignment="Top" Width="264" FontSize="25"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="10,84,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <RichTextBox HorizontalAlignment="Left" Height="187" Margin="10,109,0,0" VerticalAlignment="Top" Width="264">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
